{
    "extends": "airbnb-base",
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
        ],
        "linebreak-style": [
            "error", "windows"
        ],
        "object-curly-newline": ["off"],
        "no-unused-vars": ["warn"],

        "no-multiple-empty-lines": ["warn"]
        
   }
}
